      "edgeToEdgeEnabled": true,
      "package": "com.m_umer_m.tarotstation"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "b6850da6-a807-47b0-824f-127823963426"
